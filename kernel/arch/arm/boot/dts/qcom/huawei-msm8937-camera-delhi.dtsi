&tlmm {
	cam_sensor_mclk0_default_delhi: cam_sensor_mclk0_default_delhi {
		/* MCLK0 */
		mux {
			/* CLK, DATA */
			pins = "gpio26";
			function = "cam_mclk";
		};

		config {
			pins = "gpio26";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};

	cam_sensor_mclk0_sleep_delhi: cam_sensor_mclk0_sleep_delhi {
		/* MCLK0 */
		mux {
			/* CLK, DATA */
			pins = "gpio26";
			function = "cam_mclk";
		};

		config {
			pins = "gpio26";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
		};
	};

	cam_sensor_mclk2_default_delhi: cam_sensor_mclk2_default_delhi {
		/* CAMIF_MCLK2 */
		mux {
			/* CLK, DATA */
			pins = "gpio28";
			function = "cam_mclk";
		};

		config {
			pins = "gpio28";
			bias-disable; /* No PULL */
			drive-strength = <4>; /* 4 MA */
		};
	};

	cam_sensor_mclk2_sleep_delhi: cam_sensor_mclk2_sleep_delhi {
		/* CAMIF_MCLK2 */
		mux {
			/* CLK, DATA */
			pins = "gpio28";
			function = "cam_mclk";
		};

		config {
			pins = "gpio28";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <4>; /* 4 MA */
		};
	};

};
&soc {
	/delete-node/ qcom,camera-flash;
        led_flash0: qcom,camera-flash {
                cell-index = <0>;
                compatible = "qcom,camera-flash";
                qcom,flash-type = <1>;
                qcom,flash-source = <&pmi8937_flash0>;
                qcom,torch-source = <&pmi8937_torch0>;
                qcom,switch-source = <&pmi8937_switch>;
        };
};

